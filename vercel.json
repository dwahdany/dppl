{
  "framework": "nextjs",
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/py/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/((?!api/py/.*).*)",
      "dest": "/"
    }
  ]
}